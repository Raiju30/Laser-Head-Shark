//Limit Movement to stay within the border of the game

x = min(x, room_width - 70);
x = max(x, 70);
y = min(y, room_height - 30);
y = max(y, 100);

useKeyboard = true;
if (useKeyboard)
{
    //Keyboard Movement Controls Begin

    if (keyboard_check(vk_up))
    {
        vspeed = -3;
    }
    else if (keyboard_check(vk_down))
    {
        vspeed = 3;
    }

    if (keyboard_check(vk_left))
    {
        hspeed = -2;
    }
    else if (keyboard_check(vk_right))
    {
        hspeed = 3;
    }
    //Keyboard Movement Controls End
}
else
{
    //Mouse Movement Controls Begin
    //Stops sprite from shaking
    distFromMouse = point_distance(x,y,mouse_x,mouse_y);
    if (distFromMouse &lt; 5)
    {
        speed = 0;
    }
    else
    {
       speed = 3;
       direction = point_direction(x,y,mouse_x,mouse_y); 
    }
    

    //Mouse Movement Controls End
}
//Player Death
image_alpha = healthPoints/100;
if healthPoints &lt;= 0
{   
